{
    "name": "aldrinmagno/builder",
    "description": "a web application builder based on microservices architecture",
    "license": "MIT",
    "type": "project",
    "keywords": ["slim-framework", "builder", "mvc", "microservices", "web app builder"],
    "autoload": {
        "psr-4": {
            "Src\\Controller\\": "src/controllers",
            "Src\\Model\\": "src/models",
            "Src\\Middleware\\": "src/middleware",
            "Src\\View\\": "src/view"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/csrf": "^0.8.1",
        "zeuxisoo/slim-whoops": "^0.6.0",
        "monolog/monolog": "^1.22",
        "aura/session": "^2.1",
        "vlucas/phpdotenv": "^2.4",
        "aura/sqlquery": "^2.7",
        "aura/sql": "^2.5",
        "slim/flash": "^0.2.0",
        "jenssegers/optimus": "^0.2.3",
        "siriusphp/validation": "^2.2",
        "codeguy/upload": "^1.3",
        "slim/twig-view": "^1.2",
        "swiftmailer/swiftmailer": "^6.0",
        "nesbot/carbon": "^1.22",
        "spatie/image-optimizer": "^1.0"
    }
}
